            <solid android:color="#1A1E6BFF" /> <stroke android:width="1dp" android:color="#331E6BFF" />
        </shape>
    </item>

    <item android:top="20dp" android:bottom="20dp" android:left="20dp" android:right="20dp">
        <shape android:shape="oval">
            <solid android:color="#121B2D" /> <stroke android:width="1.5dp" android:color="#4D1E6BFF" />
        </shape>
    </item>

</layer-list>